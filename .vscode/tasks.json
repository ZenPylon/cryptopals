{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc-13 build active file",
            "command": "/usr/local/bin/gcc-13",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}.a",
                "-I",
                "/usr/local/opt/openssl@3.2/include",
                "../common/encodings.c",
                "../common/bytes.c",
                "../common/padding.c",
                "../common/cbc.c",
                "-L/usr/local/Cellar/openssl@3/3.2.1/lib/",
                "-lcrypto",
                "-lm"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}